{
  "expo": {
    "name": "E-Sky",
    "slug": "e-sky",
    "version": "1.0.0",
    "icon": "./assets/images/E-Sky-Logo.jpg",
    "scheme": "e-sky",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        
        {
          "image": "./assets/images/E-Sky-Logo.jpg",
          "imageWidth": 75,
          "resizeMode": "contain"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/E-Sky-Logo.jpg",
      "androidMode": "default"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/E-Sky-Logo.jpg",
        "backgroundColor": "#FFFFFF",
        "width": 50
      }
    },
    "ios": {
      "supportsTablet": true
    }
  }
}